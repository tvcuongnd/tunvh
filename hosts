# Điền đúng cấu trúc [name_server] ansible_host=[IP-address] nếu muốn khởi tạo file hosts

[servers]
kubespray   ansible_host=192.168.145.100 ansible_ssh_user=tvcuongnd ansible_ssh_pass="123abc@A" ansible_become_password="123abc@A" 

[servers:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'